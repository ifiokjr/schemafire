{
  "private": true,
  "name": "twitter-clone-example",
  "description": "An example api consuming the libraries in this project",
  "license": "MIT",
  "author": "Ifiok Jr. <ifiokotung@gmail.com>",
  "repository": "https://github.com/ifiokjr/schemafire/tree/examples/twitter-clone",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "run-s clean lint",
    "build": "run-s build:babel",
    "build:babel": "babel src --out-dir lib --extensions \".ts\" --source-maps true",
    "clean": "rimraf lib secret.json",
    "copy:db": "cp ../support/secrets/db.json db.json",
    "copy:secret": "cp ../support/secrets/key.json secret.json",
    "dev": "run-p watch:*",
    "lint": "tslint --project tsconfig.json --config ../../tslint.json",
    "test": "jest",
    "test:live": "cross-env TEST_ENV=live jest",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "watch:babel": "babel src --out-dir lib --extensions \".ts\" --watch"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "3.0.1",
    "@schemafire/core": "0.0.1-alpha.3",
    "@schemafire/firestore": "0.0.1-alpha.3",
    "express": "4.16.4",
    "firebase-admin": "7.0.0",
    "firebase-functions": "2.2.0",
    "glob": "7.1.3",
    "got": "9.6.0",
    "io-ts": "1.8.2",
    "lodash": "4.17.11"
  },
  "engines": {
    "node": "8"
  }
}
