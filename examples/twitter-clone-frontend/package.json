{
  "private": true,
  "name": "twitter-clone-frontend",
  "version": "0.0.0",
  "main": "./dist/server/index.js",
  "browser": {
    "firebase-admin": false,
    "@kj/plejio-server-core": false
  },
  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=browser next build",
    "build": "run-s build:*",
    "build:next": "next build",
    "build:server": "babel . --out-dir dist --extensions .ts --only server,utils,redux,routes,start.ts,typings,workers --ignore typings/globals.d.ts **/__tests__/** -s",
    "build:setup": "run-s clean typecheck lint",
    "clean": "rimraf .next dist",
    "deploy": "run-s lockfile:add build deploy:cmd lockfile:remove",
    "deploy:cmd": "echo not yet implemented",
    "dev": "run-p watch:* nodemon",
    "lint": "run-s \"lint:* {1}\" --",
    "lint:next": "tslint --project tsconfig.json --config tslint.json",
    "lint:server": "tslint --project tsconfig.server.json --config tslint.json",
    "lockfile:add": "cp -p ../../yarn.lock yarn.lock",
    "lockfile:remove": "rimraf yarn.lock",
    "nodemon": "nodemon",
    "start": "cross-env NODE_ENV=production node dist/start.js",
    "test": "jest",
    "test:live": "cross-env TEST_ENV=live jest",
    "typecheck": "run-p \"typecheck:* {1}\" --",
    "typecheck:next": "tsc -p tsconfig.json",
    "typecheck:server": "tsc -p tsconfig.server.json",
    "typecheck:workers": "tsc -p workers/tsconfig.json",
    "watch:server": "babel . --out-dir dist --extensions .ts --only server,utils,redux,routes,start.ts,typings,workers -s --watch"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/css": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@firebase/functions-types": "0.2.2",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "accept-language-parser": "1.5.0",
    "firebase-functions": "2.2.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "unstated": "2.1.1",
    "debug": "4.1.1",
    "express": "4.16.4",
    "firebase": "5.8.2",
    "firebase-admin": "7.0.0",
    "lodash": "4.17.11",
    "next": "8.0.0",
    "next-compose-plugins": "2.1.1",
    "next-offline": "3.3.5",
    "next-routes": "1.4.2",
    "ramda": "0.26.1",
    "react": "16.8.1",
    "react-albus": "2.0.0",
    "react-dom": "16.8.1",
    "rxfire": "3.3.2",
    "rxjs": "6.4.0",
    "source-map-support": "0.5.10",
    "styled-components": "4.1.3",
    "nanoid": "2.0.1",
    "@schemafire/core": "0.0.1-alpha.3"
  }
}
